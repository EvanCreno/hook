{
  "name": "doubleleft/dl-api",
  "description": "Doubleleft backend API",
  "keywords": ["doubleleft", "api"],
  "license": "MIT",
  "authors": [{
    "name": "Endel Dreyer",
    "email": "edreyer@doubleleft.com"
  }],
  "autoload": {
    "classmap": [
      "app/core",
      "app/middlewares"
    ],
    "psr-0": {
      "Models\\" : "app/"
    }
  },
  "require": {
    "composer/composer": "*",
    "slim/slim": "*",
    "doctrine/dbal" : "2.*",
    "illuminate/database": "4.1.*",
    "illuminate/config": "4.1.*",
    "illuminate/cache": "4.1.*",
    "jenssegers/mongodb": "*"
  },
  "require-dev": {
    "mockery/mockery": "0.8.0",
    "phpunit/phpunit": "3.7.*",
    "aws/aws-sdk-php": "2.*"
  },
  "bin-dir": "bin",
  "minimum-stability": "dev"
}
